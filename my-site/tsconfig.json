{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@styles/*": [
        "./styles/*"
      ],
      "@database/*": [
        "./database/*"
      ],
      "@server/*": [
        "./server/*"
      ],
      "@routes/*": [
        "./server/routes/*"
      ]
    },
    "jsx": "preserve",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "pages/api/contact.tsx"  ],
  "exclude": [
    "node_modules"
  ]
}
